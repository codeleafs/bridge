{
  "name": "{{ name }}",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "qs": "^6.5.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "1.1.4",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "rxjs": "^6.2.0"
  },
  "scripts": {
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "start": "react-app-rewired start",
    "test": "react-app-rewired test --env=jsdom",
    "build": "react-app-rewired build",
    "deploy": "PUBLIC_URL=//cdn.company.com GENERATE_SOURCEMAP=false npm run build",
    "converage": "npm test -- --coverage",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer build/static/js/main.* & ENV_ANALYZE=true npm run build",
    "storybook": "NODE_PATH=./src start-storybook -p 9009 -s public,src",
    "build-storybook": "build-storybook -s public"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.1.3",
    "@storybook/addon-actions": "^3.3.15",
    "@storybook/addon-info": "^3.4.0",
    "@storybook/addon-knobs": "^3.4.0",
    "@storybook/addon-links": "^3.3.15",
    "@storybook/addon-options": "^3.4.0",
    "@storybook/addon-viewport": "^3.4.0",
    "@storybook/addons": "^3.3.15",
    "@storybook/react": "^3.3.15",
    "babel-core": "^6.26.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "husky": "^0.14.3",
    "jest-enzyme": "^6.0.0",
    "lint-staged": "^7.1.2",
    "mock-local-storage": "^1.0.5",
    "node-sass": "^4.9.0",
    "prettier": "^1.11.1",
    "prop-types": "^15.6.1",
    "react-app-rewired": "^1.5.0",
    "react-test-renderer": "^16.2.0",
    "sass-loader": "^6.0.7",
    "source-map-explorer": "^1.5.0",
    "style-resources-loader": "^1.1.0",
    "webpack-jarvis": "^0.3.2",
    "webpack-subresource-integrity": "^1.1.0-rc.4"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --config .prettierrc --write",
      "git add"
    ],
    "package.json": [
      "prettier --config .prettierrc --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "!src/{routes,stories,api,store,assets}/**/*.*",
      "!src/*.*"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/test/setup.js",
    "collectCoverage": true,
    "coverageDirectory": "",
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    }
  },
  "proxy": {
    "/api": {
      "target": "https://company.com/",
      "changeOrigin": true
    }
  }
}
